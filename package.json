{
  "name": "justloads",
  "version": "1.0.0",
  "description": "simple module for just loading the stuff you neet",
  "main": "index.js",
  "scripts": {
    "watch": "rollup -c --watch --environment NODE_ENV:delevopment",
    "buildDev": "rollup -c --environment NODE_ENV:delevopment",
    "buildPrd": "rollup -c --environment NODE_ENV:production",
    "build": "npm-run-all buildDev buildPrd",
    "dev": "npm-run-all --parallel watch browse",
    "browse": "browser-sync start --server examples --serveStatic dist --index index.html --files examples,dist --no-notify --reload-delay 200",
    "test": "test"
  },
  "keywords": [
    "loading",
    "es6",
    "rollup",
    "asynchronous",
    "performance"
  ],
  "author": "Janik Prottung",
  "license": "ISC",
  "devDependencies": {
    "babel-preset-es2015-rollup": "^3.0.0",
    "browser-sync": "^2.18.8",
    "npm-run-all": "^4.0.2",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-eslint": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2"
  },
  "dependencies": {
    "debug": "^2.6.6"
  }
}
