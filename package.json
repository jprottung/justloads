{
  "name": "justloads",
  "version": "1.0.0",
  "description": "simple module for just loading the stuff you neet",
  "author": "Janik Prottung <jprottung@chip.de>",
  "license": "MIT",
  "browser": "dist/justloads.min.js",
  "main": "dist/justloads.umd.js",
  "jsnext:main": "dist/justloads.es.js",
  "module": "dist/justloads.es.js",
  "scripts": {
    "prebuild": "eslint src",
    "build": "rollup -c config/rollup.config.js",
    "build:watch": "rollup -w -c config/rollup.config.js",
    "pretest": "rollup -c config/rollup.config.tests.js",
    "test": "mocha -r jsdom-global/register ./build/tests-bundle.js",
    "start": "browser-sync start -c ./config/bs-config.js",
    "start:dev": "npm-run-all --parallel start build:watch",
    "prepublish": "npm run build && npm test"
  },
  "keywords": [
    "loading",
    "es6",
    "rollup",
    "asynchronous",
    "performance"
  ],
  "dependencies": {},
  "devDependencies": {
    "browser-sync": "^2.18.12",
    "chai": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "jsdom": "^11.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^3.4.2",
    "npm-run-all": "^4.0.2",
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-istanbul": "^1.1.0",
    "rollup-plugin-minify": "^1.0.3",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-watch": "^3.2.2",
    "sinon": "^2.3.2",
    "source-map-support": "^0.4.15"
  }
}
