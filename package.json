{
    "name": "justloads",
    "version": "1.0.0",
    "description": "simple module for just loading the stuff you neet",
    "author": "Janik Prottung <jprottung@chip.de>",
    "license": "MIT",
    "main": "dist/lib.umd.js",
    "jsnext:main": "dist/lib.es6.js",
    "browser": "dist/justloads.min.js",
    "scripts": {
        "prebuild": "eslint src",
        "build": "rollup -c rollup.config.umd.js && rollup -c rollup.config.es6.js",
        "pretest": "rollup -c rollup.config.tests.js",
        "start": "npm run test:watch",
        "test:watch": "nodemon --watch src --watch tests --exec 'npm test'",
        "test": "mocha build/tests-bundle.js",
        "prepublish": "npm run build && npm test"
    },
    "keywords": [
        "loading",
        "es6",
        "rollup",
        "asynchronous",
        "performance"
    ],
    "devDependencies": {
        "chai": "^3.5.0",
        "eslint": "^3.18.0",
        "eslint-config-airbnb-base": "^11.1.1",
        "eslint-plugin-import": "^2.2.0",
        "mocha": "^3.2.0",
        "nodemon": "^1.11.0",
        "rollup": "^0.41.6",
        "rollup-plugin-buble": "^0.15.0",
        "rollup-plugin-multi-entry": "^2.0.1",
        "rollup-watch": "^3.2.2",
        "source-map-support": "^0.4.13"
    }
}
